<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Bluesource_Regression" parallel= "instances" thread-count="5">
	<parameter name="browserUnderTest" value="chrome" />
	<parameter name="environment" value="staging" />
	<parameter name="runLocation" value="remote" />
	<parameter name="browserVersion" value="" />
	<parameter name="operatingSystem" value="windows" />
	<!-- <parameter name="runLocation" value="remote"</parameter> -->
	<test name="Test">
		<groups>
			<run>
				<include name="regression" />
			</run>
		</groups>
		<packages>
			<package name="com.orasi.bluesource.features">
			</package>
		</packages>
		
	</test> <!-- Test -->
</suite> <!-- Suite -->